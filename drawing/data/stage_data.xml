<?xml version="1.0" encoding="utf-8"?>

<odoo noupdate="1">
    <record model="project.item.type" id="stage_1">
        <field name="name">New</field>
        <field name="probability">10</field>
        <field name="on_change">1</field>
        <field name="sequence">1</field>
    </record>
    <record model="project.item.type" id="stage_2">
        <field name="name">Production</field>
        <field name="probability">30</field>
        <field name="on_change">1</field>
        <field name="sequence">2</field>
    </record>
    <record model="project.item.type" id="stage_3">
        <field name="name">Delivery</field>
        <field name="probability">70</field>
        <field name="on_change">1</field>
        <field name="sequence">3</field>
    </record>
    <record model="project.item.type" id="stage_4">
        <field name="name">Erection</field>
        <field name="fold" eval="False"/>
        <field name="probability">100</field>
        <field name="on_change">1</field>
        <field name="sequence">70</field>
    </record>
    <record model="project.item.type" id="stage_5">
        <field name="name">Finished</field>
        <field name="fold" eval="False"/>
        <field name="probability">100</field>
        <field name="on_change">1</field>
        <field name="sequence">70</field>
    </record>
</odoo>